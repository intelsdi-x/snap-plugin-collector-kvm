{
  "version": 1,
  "schedule": {
    "type": "simple",
    "interval": "1s"
  },
  "workflow": {
    "collect": {
      "metrics": {
        "/intel/*": {}
      },
      "config": {
        "/intel/kvm": {
          "sys_path": "/sys"
        }
      },
      "process": null,
      "publish": [
        {
          "plugin_name": "file",
          "config": {
            "file": "/tmp/published_kvm.log"
          }
        }
      ]
    }
  }
}
